{
  "version": 2,
  "builds": [{
    "src": "dist/app.js",
    "use": "@vercel/node"
  }
  ],
  "buildCommand": "pnpm run build:server",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/.*", "dest": "dist/app.js"
    }
  ],
  "crons": [
    {
      "path": "/reload-database",
      "schedule": "0 0 * * *"
    }
  ]
}